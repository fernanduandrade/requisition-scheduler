{
  "name": "requisitions",
  "version": "1.0.0",
  "description": "Simple web app for register exams requisition",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "watch:main:sass": "node-sass ./src/Sass/main/main.scss ./src/Public/css/main/style.css -w",
    "watch:login:sass": "node-sass ./src/Sass/login/main.scss ./src/Public/css/login/style.css -w",
    "server": "nodemon --exec babel-node ./index.js",
    "dev": "npm-run-all --parallel server watch:main:sass",
    "test": "jest --detectOpenHandles --silent=false"
  },
  "keywords": [
    "javascript",
    "ejs",
    "node",
    "mongodb",
    "express",
    "sass",
    "jest"
  ],
  "author": "Luiz Fernando Andrade Abreu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.12.13",
    "dotenv": "^8.2.0",
    "jest": "^27.3.0",
    "morgan": "^1.10.0",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "g": "^2.0.1",
    "mongoose": "^5.11.11",
    "nodemailer": "^6.6.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "type": "module"
}
